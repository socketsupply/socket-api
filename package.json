{
  "name": "@socketsupply/io",
  "version": "0.1.18",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint": "standardx -v",
    "readme": "node ./bin/index.js",
    "test": "cd test && npm install --silent --no-audit && npm test --silent",
    "test:node": "node ./test/node/index.js",
    "test:android": "cd test && npm install --silent --no-audit && npm run test:android --silent",
    "test:android-emulator": "cd test && npm install --silent --no-audit && npm run test:android-emulator --silent",
    "test:clean": "cd test && rm -rf dist",
    "pub": "npm pub && npm publish --registry https://npm.pkg.github.com"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socketsupply/io.git"
  },
  "bugs": {
    "url": "https://github.com/socketsupply/io/issues"
  },
  "homepage": "https://github.com/socketsupply/io#readme",
  "description": "javascript sugar on top of socket-sdk ipc",
  "devDependencies": {
    "acorn": "8.8.0",
    "acorn-walk": "8.2.0",
    "esbuild": "0.14.49",
    "tapzero": "*"
  },
  "browser": {
    "fs": "./sdk/fs.js",
    "os": "./sdk/os.js",
    "dgram": "./sdk/dgram.js",
    "process": "./sdk/process.js"
  },
  "dependencies": {
    "buffer": "6.0.3"
  }
}
